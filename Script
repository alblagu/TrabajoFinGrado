create table USUARIO
(
    DNI VARCHAR(15) not null,
    PASSWORD VARCHAR(20) not null,
    NOMBRE VARCHAR(30) not null,
    APELLIDOS VARCHAR(50) not null,
    TELEFONO INT not null,
        PRIMARY KEY(DNI)
);

create table LIBRO
(
  ISBN10 VARCHAR(30) not null,
  ISBN13 VARCHAR(30) not null,
  TITULO VARCHAR(100) not null,
      PRIMARY KEY(ISBN10),
      UNIQUE (ISBN13)
);

create table EJEMPLAR
(
  CODIGO VARCHAR(30) not null,
  LIBRO VARCHAR(30) not null,
      PRIMARY KEY(CODIGO),
      FOREIGN KEY(LIBRO) REFERENCES LIBRO(ISBN10)
);

create table PRESTAMO
(
    ID INT not null,
    USUARIO VARCHAR(15) not null,
    EJEMPLAR VARCHAR(30) not null,
    FECHA_INI DATE not null,
    FECHA_FIN DATE not null,
        PRIMARY KEY(ID),
        FOREIGN KEY(USUARIO) REFERENCES USUARIO(DNI),
        FOREIGN KEY(EJEMPLAR) REFERENCES EJEMPLAR(CODIGO)

);



INSERT INTO LIBRO VALUES ('1','2','blancanieves' );
